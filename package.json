{
  "name": "ralltiir",
  "version": "2.7.0-2",
  "discription": "前端极速浏览框架，目标是提升用户体验，提供沉浸式浏览方式。",
  "scripts": {
    "doc-start": "gitbook serve --port 8883 docs",
    "doc-deploy": "make doc-deploy",
    "test": "make test",
    "test-watch": "make test-watch",
    "test-reports": "make test-reports",
    "preversion": "npm run lint && npm test",
    "postversion": "git push origin master && git push --tags",
    "clean": "make clean",
    "lint": "fecs check src, test",
    "build": "make build",
    "version": "npm run dist",
    "dist": "make dist"
  },
  "main": "./dist/ralltiir.min.js",
  "files": [
    "dist/ralltiir.js",
    "dist/ralltiir.min.js"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "fecs": "^1.2.2",
    "fis3": "^3.4.33",
    "fis3-hook-amd": "^0.2.0",
    "fis3-hook-commonjs": "^0.1.25",
    "gitbook-cli": "^2.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-html-reporter": "^0.2.7",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^3.2.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0"
  }
}
